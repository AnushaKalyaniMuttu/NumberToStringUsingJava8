package com.bean;

import java.util.Arrays;
import java.util.Collections;
import java.util.List;
import java.util.stream.Collectors;

public class Test {
    

		    public static void main(String[] args) {
		   
		    List<Integer> list=Arrays.asList(7,3,9,3,1,8,9);
	String value= list.stream().sorted(Collections.reverseOrder()).map(Object::toString).collect(Collectors.joining());
		 System.out.println(value);
		}
	
	
}
